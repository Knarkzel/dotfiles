map d delete
map D delete

cmd aj %lf -remote "send $id cd '$(autojump $1)'"
map a push :aj<space>
map x :cut
map c :copy
map u push :clear<enter>:unselect<enter>
map i :invert
map gl push :glob-select<space>**<left>
map gi $nvim -c "Git"
map w :quit

map f $nvim $(fzf)
map e $nvim $f
map l $nvim $f

map . :set hidden!
